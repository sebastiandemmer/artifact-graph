    "ts-node": "10.8.0",
    "typescript": "^4.9.5",
    "webpack": "^5.82.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.13.3",
    "wepack-cli": "^0.0.1-security",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "coverage",
    "testEnvironment": "node",
    "testTimeout": 5000
  }
}
