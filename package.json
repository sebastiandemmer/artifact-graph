{
  "name": "artifact-graph",
  "version": "0.0.2",
  "description": "Artifact Graph project",
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "url": "git+https://github.com/sebastiandemmer/artifact-graph.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "contributors": [],
  "author": "",
  "license": "Apache-2.0",
  "scripts": {
    "build": "webpack --mode=production --node-env=production",
    "build:release": "tsc && webpack",
    "webpack": "webpack",
    "webpack:watch": "webpack --watch",
    "serve": "webpack serve",
    "test": "jest --runInBand --detectOpenHandles --forceExit --verbose --useStderr",
    "coverage": "npm test -- --coverage --collectCoverageFrom='./src/**'",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "release": "semantic-release --branches main",
    "prepare": "husky install",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "predeploy": "npm run build",
    "deploy-gh-pages": "gh-pages -d dist"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@memgraph/orb": "^0.3.0",
    "@popperjs/core": "^2.11.7",
    "autoprefixer": "^10.4.14",
    "bootstrap": "^5.2.3",
    "d3-drag": "3.0.0",
    "d3-ease": "3.0.1",
    "d3-force": "3.0.0",
